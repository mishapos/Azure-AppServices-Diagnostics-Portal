    "protractor": "~7.0.0",
    "rxjs-tslint": "^0.1.3",
    "selfsigned": "^1.10.2",
    "ts-node": "~4.1.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.8",
    "webpack-bundle-analyzer": "^3.6.0"
  }
}
