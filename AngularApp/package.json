{
  "name": "azure-web-apps-support-center",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "applens": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng serve applens --aot --optimization=false",
    "ncloud-applens-prod": "ng serve applens --configuration=nationalcloud-production --aot --optimization=false",
    "ncloud-applens-local": "ng serve applens --configuration=nationalcloud-local --aot --optimization=false",
    "ssl": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng serve --host 0.0.0.0 app-service-diagnostics --aot --optimization=false --ssl --sslCert ./ssl/server.crt --sslKey ./ssl/server.key",
    "ssl-local": "npm run ssl -- -c local",
    "asd": "concurrently -k \"npm run ssl\" \"npm run lib\"",
    "build-lib": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build diagnostic-data",
    "build-applens": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build applens --prod",
    "build-ncloud-applens-prod": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build applens --configuration=nationalcloud-production",
    "build-ncloud-applens-local": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build applens --configuration=nationalcloud-local",
    "build-asd": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build app-service-diagnostics --prod --deploy-url https://app-service-diagnostics-portal.azureedge.net/ --sourceMap=true",
    "build": "npm run build-lib && npm run build-applens && npm run build-asd",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "lib": "ng build diagnostic-data --watch",
    "generate-report": "ng build app-service-diagnostics --prod",
    "bundle-report": "webpack-bundle-analyzer ../Backend/wwwroot/stats.json -p 8889"
  },
  "dependencies": {
    "@angular-devkit/core": "^12.2.17",
    "@angular/common": "11.2.14",
    "@angular/compiler": "11.2.14",
    "@angular/core": "11.2.14",
    "@angular/platform-browser-dynamic": "11.2.14",
    "@angular/router": "11.2.14",
    "@angular/upgrade": "11.2.14",
    "@microsoft/applicationinsights-web": "^2.5.4",
    "@types/highcharts": "^7.0.0",
    "@types/react": "~16.8.6",
    "@types/react-dom": "^16.8.6",
    "@uifabric/azure-themes": "7.3.0",
    "adal-angular4": "^3.0.16",
    "admin-lte": "^2.4.3",
    "angular-split-ng6": "^1.0.0-rc.5",
    "bootstrap": "^3.4.1",
    "copy-html-to-clipboard": "^4.0.1",
    "core-js": "^3.6.5",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "highcharts": "^8.1.2",
    "highcharts-angular": "^2.4.0",
    "highcharts-custom-events": "^3.0.9",
    "ie-shim": "^0.1.0",
    "jquery": "^3.4.1",
    "moment": "^2.22.1",
    "monaco-languageclient": "^0.13.0",
    "ng-in-viewport": "^6.1.0",
    "ngx-monaco-editor": "^8.1.1",
    "node-sass": "^4.14.0",
    "pdfmake": "^0.1.72",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "reconnecting-websocket": "^4.4.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "6.6.7",
    "rxjs-compat": "6.2.2",
    "tslib": "^2.0.0",
    "ua-parser-js": "^0.7.19",
    "vis": "^4.21.0",
    "vscode-ws-jsonrpc": "^0.2.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.10.2"
